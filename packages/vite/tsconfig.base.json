{
  "compilerOptions": {
    "rootDir": ".",
    "target": "ES2023",
    "module": "Preserve",
    "types": ["node"],
    "noEmit": true,
    "moduleResolution": "bundler",
    "strict": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false
  }
}
