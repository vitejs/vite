  "homepage": "https://github.com/vitejs/vite/tree/main/packages/plugin-vue-jsx#readme",
  "dependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@vue/babel-plugin-jsx": "^1.1.1"
  },
  "devDependencies": {
    "vite": "workspace:*"
