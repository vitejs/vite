  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "browserslist": "^4.24.2",
    "browserslist-to-esbuild": "^2.1.1",
    "core-js": "^3.38.1",
